{
  "version": 2,
  "builds": [
    {
      "src": "api/index.js",
      "use": "@vercel/node"
    },
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist",
        "cleanUrls": true
      }
    }

  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/index.js",
      "continue": true
    },
    {
      "src": "/(.*)",
      "dest": "/dist/$1",
      "headers": {
        "Cache-Control": "s-maxage=0, stale-while-revalidate=60"
      },
      "continue": true
      
    }
    
  ]
}