{
    "version": 2,
    "builds": [
      {
        "src": "api/index.js",
        "use": "@vercel/node"
      }
    ],
    "rewrites": [
      {
        "source": "/(.*)",
        "destination": "/"
      },
      {
        "source": "/api/(.*)",
        "destination": "/api/index.js"
      }
    ]
    
}